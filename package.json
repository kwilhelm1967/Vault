{
  "name": "local-password-vault",
  "private": true,
  "version": "1.2.0",
  "main": "electron/main.js",
  "author": {
    "name": "Local Password Vault",
    "email": "support@localpasswordvault.com"
  },
  "description": "Professional offline password manager with local encryption and modern UI",
  "keywords": [
    "password-manager",
    "security",
    "encryption",
    "offline",
    "privacy",
    "desktop"
  ],
  "author": "Local Password Vault Team",
  "repository": {
    "type": "git",
    "url": "https://github.com/kwilhelm1967/Vault"
  },
  "bugs": {
    "url": "https://github.com/kwilhelm1967/Vault/issues"
  },
  "homepage": "https://localpasswordvault.com",
  "scripts": {
    "dev": "npm run dev:lpv",
    "dev:lpv": "concurrently \"npm run dev:vite:lpv\" \"npm run dev:electron:lpv\"",
    "dev:vite": "vite --mode lpv",
    "dev:vite:lpv": "vite --mode lpv --port 5173",
    "dev:vite:llv": "vite --mode llv --port 5174",
    "dev:llv": "concurrently \"npm run dev:vite:llv\" \"npm run dev:electron:llv\"",
    "dev:both": "concurrently \"npm run dev:vite:lpv\" \"npm run dev:vite:llv\"",
    "dev:electron": "wait-on tcp:5173 && nodemon",
    "dev:electron:lpv": "wait-on tcp:5173 && nodemon",
    "dev:electron:llv": "wait-on tcp:5174 && nodemon --exec \"electron . --legacy\"",
    "start:test": "vite --mode test",
    "start": "echo 'Use npm run dev to start development server'",
    "build": "vite build",
    "build-only": "vite build",
    "build:prod": "vite build --mode production",
    "electron": "wait-on tcp:5173 && electron .",
    "electron-dev": "npm run dev",
    "electron-test": "concurrently \"npm run start:test\" \"npm run electron\"",
    "electron-pack": "electron-builder",
    "dist": "npm run build-only && electron-builder --publish=never",
    "dist:prod": "npm run build:prod && electron-builder --publish=never",
    "dist:win": "npm run build:prod && electron-builder --win --publish=never",
    "dist:mac": "npm run build:prod && electron-builder --mac --publish=never",
    "dist:linux": "npm run build:prod && electron-builder --linux --publish=never",
    "dist:all": "npm run build:prod && electron-builder -mwl --publish=never",
    "release": "npm run build:prod && electron-builder --publish=always",
    "release:win": "npm run build:prod && electron-builder --win --publish=always",
    "release:mac": "npm run build:prod && electron-builder --mac --publish=always",
    "lint": "eslint .",
    "preview": "vite preview",
    "preview:lpv": "vite preview --port 4174 --host",
    "trial-flow": "powershell -ExecutionPolicy Bypass -File ./scripts/run-trial-flow.ps1",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "get-lpv-from-github": "node scripts/get-lpv-from-github.js"
  },
  "dependencies": {
    "@sentry/react": "^8.0.0",
    "@stripe/react-stripe-js": "^3.7.0",
    "@stripe/stripe-js": "^7.4.0",
    "@types/jspdf": "^1.3.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "electron-log": "^5.4.3",
    "electron-positioner": "^4.1.0",
    "electron-updater": "^6.6.2",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "i18next": "^25.7.1",
    "jspdf": "4.1.0",
    "lucide-react": "^0.542.0",
    "nodemailer": "^6.9.9",
    "qrcode": "^1.5.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^16.3.5",
    "recharts": "^2.15.4",
    "stripe": "^18.3.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@eslint/js": "^9.9.1",
    "@playwright/test": "^1.57.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/qrcode": "^1.5.6",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "archiver": "^7.0.1",
    "autoprefixer": "^10.4.18",
    "babel-jest": "^30.2.0",
    "babel-plugin-transform-import-meta": "^2.3.3",
    "concurrently": "^8.2.2",
    "electron": "^28.3.3",
    "electron-builder": "^24.13.3",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "identity-obj-proxy": "^3.0.0",
    "javascript-obfuscator": "^4.0.2",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "nodemon": "^3.0.2",
    "postcss": "^8.4.35",
    "sharp": "^0.34.5",
    "tailwindcss": "^3.4.1",
    "to-ico": "1.0.1",
    "ts-jest": "^29.4.6",
    "ts-jest-mock-import-meta": "^1.3.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "unzipper": "^0.10.14",
    "vite": "^5.4.2",
    "vite-plugin-electron": "^0.15.5",
    "vite-plugin-obfuscator": "^1.0.3",
    "wait-on": "^7.2.0"
  },
  "overrides": {
    "tar": ">=7.5.7",
    "qs": ">=6.14.1"
  }
}
